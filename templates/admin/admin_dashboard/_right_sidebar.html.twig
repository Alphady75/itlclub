<div class="card mb-3 color-bg-200">
    <div class="card-body">
        <div class="card-body bg-primary rounded-3 mb-4">
            <div class="info-watch d-flex justify-content-between align-items-center">
                <span class="text-white fw-bold">Derni√®res demandes</span>
            </div>
            <ul class="list-unstyled list-group list-group-custom list-group-flush mb-0 mt-3">
                {% for demande in lastdemandes %}
                <li class="list-group-item py-3 bg-primary">
                    <div class="d-flex align-items-center">
                        <div class="avatar lg bg-light rounded-circle no-thumbnail"><i class="icofont-man-in-glasses fs-6"></i></div>
                        <div class="flex-fill ms-3">
                            <span class="text-white fw-bold">{{ demande.created|date('d/m/Y')}}</span>
                            <h6 class="mb-0 text-white">{{ demande.user.name ~ ' ' ~ demande.user.lastname }}</h6>
                        </div>
                    </div>
                </li>
                {% else %}
                <li></li>
                {% endfor %}
            </ul>
            <a href="{{ path('admin_demande_index') }}" class="btn btn-primary shadow shadow-sm">Administrer</a>
        </div>
