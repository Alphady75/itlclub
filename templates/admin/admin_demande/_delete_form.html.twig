<form class="d-inline" method="post" action="{{ path('admin_demande_delete', {'id': demande.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ demande.id) }}">
    <button class="btn btn-danger text-light btn-sm"><i class="fa fa-trash"></i></button>
</form>
