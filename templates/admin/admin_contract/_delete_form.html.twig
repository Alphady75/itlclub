<form method="post" class="mt-3 d-inline" action="{{ path('admin_contract_delete', {'id': contract.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contract.id) }}">
    <button class="btn btn-danger text-light"><i class="fa fa-trash"></i> Supprimer</button>
</form>
