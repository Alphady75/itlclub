{% extends 'admin.html.twig' %}

{% block title %}Adress{% endblock %}

{% block body %}
    <h1>Adress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adress.id }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ adress.number }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ adress.street }}</td>
            </tr>
            <tr>
                <th>PostalCode</th>
                <td>{{ adress.postalCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ adress.city }}</td>
            </tr>
        </tbody>
    </table>

    {% for ad in adress %}
        <p>{{ ad.street }}</p>
    {% endfor %}



    <a href="{{ path('admin_adress_index') }}">back to list</a>

    <a href="{{ path('admin_adress_edit', {'id': adress.id}) }}">edit</a>

    {{ include('admin/admin_adress/_delete_form.html.twig') }}
{% endblock %}
